{
  "site": {
    "title": "THW Braunschweig Höhenrettung",
    "base_url": "https://hoehenrettung-bs.de",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.svg",
    "logo": "/images/logo.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "45",
    "logo_height": "45",
    "logo_text": "Astroplate"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
